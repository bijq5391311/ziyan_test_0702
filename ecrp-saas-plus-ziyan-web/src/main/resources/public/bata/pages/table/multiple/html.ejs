<% include ../../../include/layout/start.ejs %>
<div id="example" v-cloak>
  <div class="template-page">
    <div class="template-page-location">
      <el-breadcrumb separator=">">
        <el-breadcrumb-item :to="{ path: '/' }">首页</el-breadcrumb-item>
        <el-breadcrumb-item>会员洞察</el-breadcrumb-item>
        <el-breadcrumb-item>会员分群</el-breadcrumb-item>
        <el-breadcrumb-item>会员划分</el-breadcrumb-item>
      </el-breadcrumb>
    </div>

    <div class="template-page-tab">
      <el-tabs v-model="activeName" type="card">
        <el-tab-pane label="交易数据" name="first">
          <div class="tmp-page__content">
            <el-row>
              <el-col :span="24">
                <el-form-grid block class="el-block-box mb-md">
                  <label>查询标签：</label>
                  <el-tag type="gray" class="is-pointer mr-sm" v-for="(tag, index) in evaluateTag" :key="index"
                          :type="tag.check ? 'primary' : 'gray'" @click.native="tagHandleClick(index, tag.check)">
                    {{tag.name}}
                  </el-tag>
                </el-form-grid>
                <div class="template-table-bar">
                  <el-row type="flex">
                    <el-col :span="12">
                      <el-form-grid>
                        <el-button type="primary"><i class="bui-edit"></i>批量备注</el-button>
                      </el-form-grid>
                      <el-form-grid>
                        <el-button type="primary"><i class="bui-send"></i>批量发送短信</el-button>
                      </el-form-grid>
                      <el-form-grid>
                        <el-button type="primary"><i class="bui-export"></i>导出数据（CSV）表格</el-button>
                      </el-form-grid>
                    </el-col>
                    <el-col :span="12">
                      <el-form :inline="true" :model="multipleForm" class="pull-right" v-if="multipleFilterStatus">
                        <el-form-item v-if="multipleSearchCollapse">
                          <el-form-grid>
                            <el-form-item label="催付商品：">
                              <el-radio-group v-model="formRadio">
                                <el-radio label="不限"></el-radio>
                                <el-radio label="排除" disabled></el-radio>
                                <el-radio label="只限"></el-radio>
                              </el-radio-group>
                            </el-form-item>
                          </el-form-grid>
                          <el-form-grid size="xmd">
                            <el-input
                              placeholder="请输入姓名"
                              icon="search"
                              v-model="multipleForm.user"
                              :on-icon-click="filterSubmit">
                            </el-input>
                          </el-form-grid>
                        </el-form-item>
                        <el-form-item>
                          <el-button type="text" @click="multipleShow" ref="multipleSearchText">展开滤选<i
                              class="el-icon-arrow-down el-icon--right"></i>
                          </el-button>
                        </el-form-item>
                      </el-form>
                      <el-form :inline="true" class="pull-right" v-else>
                        <el-form-item>
                          <el-button type="text" @click="resultShow" ref="resultCollapseText">收缩结果<i
                              class="el-icon-arrow-up el-icon--right"></i>
                          </el-button>
                        </el-form-item>
                      </el-form>
                    </el-col>
                  </el-row>
                </div>
